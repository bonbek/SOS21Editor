<flex-config>
	<benchmark>false</benchmark>
	<default-background-color>#000000</default-background-color>
	<compiler>
		<incremental>true</incremental>
		<optimize>true</optimize>
		<keep-generated-actionscript>false</keep-generated-actionscript>
		<warn-no-constructor>false</warn-no-constructor>
		<warn-no-explicit-super-call-in-constructor>false</warn-no-explicit-super-call-in-constructor>
      	<show-actionscript-warnings>true</show-actionscript-warnings>
		<show-binding-warnings>true</show-binding-warnings>
		<show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>		
		<debug>false</debug>
		
		<library-path append="true">
			<path-element>../lib/bin/</path-element>
		</library-path>
		
		<source-path append="true">
			<path-element>../../core/lib/src/</path-element>
			<path-element>../lib/src/</path-element>
		</source-path>
		
		
		
	</compiler>
	<!-- <link-report>link-report.xml</link-report> 
	<load-externs>flexmapeditor-exclude.xml</load-externs> 
	-->
	<metadata>
		<title>MapEditor Application</title>
		<description>Editeur de cartes pour le jeu dud√©veloppement durable sos-21</description>
		<publisher>Made in TextMate</publisher>
		<creator>toffer</creator>
		<language>FR</language>
	</metadata>
	<raw-metadata></raw-metadata>
</flex-config>
